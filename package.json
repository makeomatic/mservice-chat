    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.1.6",
