    "is": "^3.1.0",
    "lodash": "^4.13.1",
    "ms-conf": "^2.1.0",
    "ms-socket.io-adapter-amqp": "^1.0.0",
    "ms-validation": "^2.0.0",
    "mservice": "^2.6.0",
    "server-destroy": "^1.0.1",
    "socket.io": "^1.4.8",
    "uid2": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
