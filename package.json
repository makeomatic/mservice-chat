    "hapi": "^16.0.1",
    "is": "^3.2.0",
    "lodash": "^4.17.2",
    "mixwith": "^0.1.1",
    "moment": "^2.17.1",
    "ms-amqp-transport": "^7.0.1",
    "ms-conf": "^2.3.0",
    "ms-flakeless": "^4.0.1",
    "ms-profanity": "^1.0.1",
    "ms-socket.io-adapter-amqp": "^3.0.1",
    "ms-validation": "^3.0.0",
    "mservice": "^4.8.0",
    "node-uuid": "^1.4.7",
    "socket.io": "^1.7.2",
    "socketio-wildcard": "^0.3.0",
    "uid2": "0.0.3"
