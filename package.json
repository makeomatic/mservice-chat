{
  "name": "mservice-chat",
  "version": "0.0.0",
  "description": "mservice-chat",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "rimraf ./lib; babel -d ./lib ./src",
    "doc": "cd schemas && apidoc -i ./../src/actions -v -o ./../docs",
    "lint": "eslint . --ext .json --ext .js",
    "prepublishOnly": "yarn lint && yarn compile",
    "docker-release": "mdep docker release",
    "semantic-release": "semantic-release",
    "start": "node ./node_modules/.bin/mfleet | bunyan -o short",
    "pretest": "yarn compile",
    "test": "yarn lint && mdep test run"
  },
  "dependencies": {
    "@microfleet/core": "^13.6.0",
    "@microfleet/transport-amqp": "^13.1.3",
    "@microfleet/validation": "^8.1.1",
    "bluebird": "^3.5.0",
    "boom": "^7.3.0",
    "bunyan": "^1.8.9",
    "common-errors": "^1.0.0",
    "express-cassandra": "^2.3.0",
    "glob": "^7.1.1",
    "hapi": "^18.1.0",
    "is": "^3.2.1",
    "lodash": "^4.17.4",
    "mixwith": "^0.1.1",
    "moment": "^2.18.1",
    "ms-conf": "^3.1.2",
    "ms-flakeless": "^4.0.1",
    "ms-profanity": "^1.0.1",
    "ms-socket.io-adapter-amqp": "^5.0.0",
    "socket.io": "^2.2.0",
    "socketio-wildcard": "^2.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@makeomatic/deploy": "^8.4.1",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "apidoc": "^0.17.5",
    "apidoc-plugin-schema": "^0.1.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.2.0",
    "chance": "^1.0.6",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-promise": "^4.0.1",
    "json": "^9.0.6",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.6.1",
    "socket.io-client": "^2.2.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/mservice-chat.git"
  },
  "engines": {
    "node": ">= 10.9.0",
    "npm": ">= 5.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
